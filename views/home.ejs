<%- include('./partials/header'); -%>

<h1>Home</h1>
<p> <%= homeStartingContent %> </p>
<!--.forEach() is an better alternate to for loops-->
<% posts.forEach(function(post){ %>
    <h1> <%= post.title %></h1>
    <p> <%= post.description.substring(0,100) %>... </p>
    <a href="/posts/<%= post.title %> " >Read More</a>
<%}); %>



